{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "route": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": ["GET", "POST", "DELETE", "PATCH", "PUT", "OPTIONS"]
        }
    ]
}
